.circle_botton_center{
  width: 100px;
  height: 100px;
  border: 0 solid #ffffff;
  border-radius: 500px;
  box-shadow: 4px 1px 1px #cccccc;
  margin-top: 200px;
  display: flex;
  align-items: center;
  justify-content: center;
}

.circle_botton_right{
  width: 20px;
  height: 50px;
  border: 0 solid #ffffff;
  border-radius: 50px;
  box-shadow: 4px 1px 1px #cccccc;
  margin-top: 200px;
  margin-left: 100px;
  display: flex;
  align-items: center;
  justify-content: center;
}

.window_view {
  width: 100%;
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: flex-end;
}